pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
          environment {
              DEBUG_FLAGS = '-g'
          }
          steps {
              sh 'printenv'
          }
        }
    }
}
