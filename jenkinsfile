pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo "GIT_BRANCH: ${env.GIT_BRANCH#*origin/}"
                echo "GIT_AUTHOR_EMAIL: ${env.GIT_AUTHOR_EMAIL}"
            }
        }
        stage('Deploy') {
          environment {
              DEBUG_FLAGS = '-g'
          }
          steps {
              sh 'printenv'
          }
        }
    }
}
