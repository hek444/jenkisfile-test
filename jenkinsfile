pipeline {
    agent any
    environment {
        CC = 'clang'
    }
    stages {
        stage('Variables') {
            steps {
                echo "gitlabSourceRepoName: ${gitlabSourceRepoName}"
                echo "gitlabSourceBranch: ${gitlabSourceBranch}"
            }
        }
        stage('Example') {
            environment {
                DEBUG_FLAGS = '-g'
            }
            steps {
                sh 'printenv'
            }
        }
    }
}
